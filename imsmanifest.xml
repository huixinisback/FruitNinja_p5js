<?xml version="1.0" encoding="UTF-8"?>
<manifest identifier="com.example.myscormproject"
          version="1.0"
          xmlns="http://www.imsproject.org/xsd/imscp_rootv1p1p2"
          xmlns:adlcp="http://www.adlnet.org/xsd/adlcp_rootv1p2"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.imsproject.org/xsd/imscp_rootv1p1p2 
                              imscp_rootv1p1p2.xsd 
                              http://www.adlnet.org/xsd/adlcp_rootv1p2 
                              adlcp_rootv1p2.xsd">

  <organizations default="org1">
    <organization identifier="org1">
      <title>My SCORM Project</title>
      <item identifier="item1" identifierref="resource1">
        <title>Launch My Content</title>
      </item>
    </organization>
  </organizations>

  <resources>
    <resource identifier="resource1"
              type="webcontent"
              adlcp:scormtype="sco"
              href="index.html">
        <file href="index.html"/>
        <file href="fruitninja_advanced.js"/>
        <!-- Add your asset files here -->
        <file href="assets/peachhalf.png"/>
        <file href="assets/peachhalf1.png"/>
        <file href="assets/peachwhole.png"/>
        <file href="assets/dojobackground.png"/>
        <file href="assets/peachsplash.png"/>
        <file href="assets/watermelonhalf.png"/>
        <file href="assets/watermelonsplash.png"/>
        <file href="assets/watermelonwhole.png"/>
    </resource>
  </resources>
</manifest>
